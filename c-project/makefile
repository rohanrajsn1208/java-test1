project.exe: main.o big2.o big3.o fact.o rev.o palindrome.o sumofn.o fibonacci.o sort.o
	gcc -o project.exe main.o big2.o big3.o fact.o rev.o palindrome.o sumofn.o fibonacci.o sort.o
main.o: main.c
	gcc -c main.c
big2.o: big2.c
	gcc -c big2.c
big3.o: big3.c
	gcc -c big3.c
fact.o: fact.c
	gcc -c fact.c
rev.o: rev.c
	gcc -c rev.c
palindrome.o: palindrome.c
	gcc -c palindrome.c
sumofn.o: sumofn.c
	gcc -c sumofn.c
fibonacci.o: fibonacci.c
	gcc -c fibonacci.c
sort.o: sort.c
	gcc -c sort.c
clean:
	rm -rf *.o 

